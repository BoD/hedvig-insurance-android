<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.hedvig.app">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        tools:ignore="AllowBackup,GoogleAppIndexingWarning"
        tools:targetApi="28">

        <activity
            android:name=".DevelopmentActivity"
            android:label="Hedvig Development Screen"
            android:theme="@style/Hedvig.Theme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".VectorDrawableGalleryActivity"
            android:label="Vector Drawable Gallery"
            android:theme="@style/Hedvig.Theme" />

        <meta-data
            android:name="io.branch.sdk.BranchKey.test"
            android:value="@string/BRANCH_KEY" />
        <meta-data
            android:name="io.branch.sdk.TestMode"
            android:value="true" />

    </application>

</manifest>
