<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProfileMenuRow">
        <attr name="iconImage" format="string" />
        <attr name="name" format="string" />
        <attr name="description" format="string" />
    </declare-styleable>
    <declare-styleable name="PerilCategoryView">
        <attr name="categoryUrl" format="string" />
        <attr name="title" format="string" />
        <attr name="subtitle" format="string" />
    </declare-styleable>
    <declare-styleable name="PerilView">
        <attr name="perilIcon" format="string" />
        <attr name="perilText" format="string" />
    </declare-styleable>
    <declare-styleable name="PagerIndicatorView">
        <attr name="pager" format="reference" />
    </declare-styleable>
    <declare-styleable name="HedvigCardView">
        <attr name="android:elevation" format="dimension" />
    </declare-styleable>
    <declare-styleable name="BezierView">
        <attr name="topColor" format="color" />
        <attr name="bottomColor" format="color" />
    </declare-styleable>
    <declare-styleable name="DateInputView">
        <attr name="dateHintBackground" format="color" />
    </declare-styleable>
    <declare-styleable name="HedvigEditText">
        <attr name="hintBackground" format="color" />
        <attr name="android:imeOptions" format="flags" />
        <attr name="android:inputType" format="flags" />
        <attr name="hintText" format="string" />
    </declare-styleable>
</resources>
